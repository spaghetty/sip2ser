From 4f3843add7365b981ef8e4e9d41e9b8769ebb0e0 Mon Sep 17 00:00:00 2001
From: luciano <luciano@opsip.testhost.net>
Date: Thu, 28 Jun 2012 15:07:36 +0200
Subject: [PATCH] add-author-to-gems

---
 sipXconfig/agent/Rakefile  |    1 +
 sipXproxy/src/cdr/Rakefile |    1 +
 2 files changed, 2 insertions(+), 0 deletions(-)

diff --git a/sipXconfig/agent/Rakefile b/sipXconfig/agent/Rakefile
index dd78e50..23d567f 100644
--- a/sipXconfig/agent/Rakefile
+++ b/sipXconfig/agent/Rakefile
@@ -22,6 +22,7 @@ spec = Gem::Specification.new do |s|
   s.platform = Gem::Platform::RUBY
   s.summary = "sipXconfig-agent provides SOAP interface for various sipX components"
   s.name = 'sipXconfig-agent'
+  s.author = 'ezuce'
   s.version = '1.0.0'
   s.requirements << 'none'
   s.require_path = 'lib'
diff --git a/sipXproxy/src/cdr/Rakefile b/sipXproxy/src/cdr/Rakefile
index 21a2c55..f711d01 100644
--- a/sipXproxy/src/cdr/Rakefile
+++ b/sipXproxy/src/cdr/Rakefile
@@ -21,6 +21,7 @@ spec = Gem::Specification.new do |s|
   s.platform = Gem::Platform::RUBY
   s.summary = "sipxcallresolver provides CDR generation from logged call state events"
   s.name = 'sipxcallresolver'
+  s.author = 'ezuce'
   s.version = CALLRESOLVER_VERSION
   s.requirements << 'none'
   s.require_path = 'lib'
-- 
