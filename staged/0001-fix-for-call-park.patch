 sipXcallLib/include/cp/CpCall.h |    2 +-
 sipXcallLib/src/cp/CpCall.cpp   |    2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/sipXcallLib/include/cp/CpCall.h b/sipXcallLib/include/cp/CpCall.h
index f53ade9..2f68f60 100644
--- a/sipXcallLib/include/cp/CpCall.h
