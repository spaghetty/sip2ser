From: luciano <luciano@opsip.testhost.net>
Date: Wed, 11 Jul 2012 17:20:38 +0200
Subject: [PATCH] pwd-and-snom300-defaults

---
 sipXconfig/plugins/snom/etc/snom/snom.vm |   10 ++++++++++
 1 files changed, 10 insertions(+), 0 deletions(-)

diff --git a/sipXconfig/plugins/snom/etc/snom/snom.vm b/sipXconfig/plugins/snom/etc/snom/snom.vm
index ee44341..afc051e 100644
--- a/sipXconfig/plugins/snom/etc/snom/snom.vm
+++ b/sipXconfig/plugins/snom/etc/snom/snom.vm
@@ -14,10 +14,20 @@ perm="R"#end
 #foreach ($group in $phone.Settings.Values)
 #if ($group.Name != 'fkeys')
 #foreach ($setting in $group.Values)
+#if ($phone.Model.Label=="Snom 300" && ${setting.name} == "dhcp" && ${setting.Value}=="off")
+                <${setting.name} #perm( $setting )>$!{setting.Value}</${setting.name}>
+                <ip_adr perm="R">192.168.64.85</ip_adr>
+                <netmask perm="R">255.255.255.0</netmask>
+                <gateway perm="R">192.168.64.2</gateway>
+                <dns_domain perm="R">sip2ser.net</dns_domain>
+                <dns_server1 perm="R">192.168.64.1</dns_server1>
+                <dns_server2 perm="R"></dns_server2>
+#else
 		<${setting.name} #perm( $setting )>$!{setting.Value}</${setting.name}>
 #end
 #end
 #end
+		<ignore_security_warning perm="R">on</ignore_security_warning>
 		<dkey_retrieve perm="R">speed 101</dkey_retrieve>
 
 		<!-- speeddial list -->
