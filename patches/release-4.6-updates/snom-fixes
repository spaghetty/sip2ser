+          <option><value>Italiano</value></option>
+          <option><value>Nederlands</value></option>
+          <option><value>Norsk</value></option>
+          <option><value>Poliski</value></option>
+          <option><value>Portugues</value></option>
+          <option><value>Russian</value></option>
+          <option><value>Slovencina</value></option>
+          <option><value>Suomi</value></option>
+          <option><value>Svenska</value></option>
+          <option><value>Turkce</value></option>
+        </enum>
+      </type>
+      <value>Italiano</value>
+    </setting>  
+
+    <setting name="version">
+      <type>
+        <enum>
+          <option><value>7.3.X</value></option>
+          <option><value>8.4.X</value></option>
+        </enum>
+      </type>
+      <value>7.3.X</value>
+    </setting>
+  </group>
+  <group name="firmware" hidden="yes">
