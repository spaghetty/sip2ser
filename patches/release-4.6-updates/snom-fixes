+<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
+  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
+<model>
+  <type id="zip">
+    <file>
+      <contentType>application/zip</contentType>
+    </file>
+  </type>
+  <type id="binary">
+    <file>
+      <contentType>application/binary</contentType>
+    </file>
+  </type>
+  <group name="localization">
+    <setting name="file">
+      <type>
+        <file>
+          <contentType>application/xml</contentType>
+        </file>
+      </type>
+    </setting>
+    <setting name="context">
+      <type>
+	<enum> 
+	  <option><value>Web</value></option>
+	  <option><value>Gui</value></option>
+	</enum>
+      </type>
+      <value>Web</value>
+    </setting>
+    <setting name="language">
+      <type>
+        <enum>
+          <option><value>Catalan</value></option>
+          <option><value>Cestina</value></option>
